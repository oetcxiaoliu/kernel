#
# rockchip device configuration
#

config NET_VENDOR_ROCKCHIP
        bool "Rockchip devices"
        default y
        depends on HAS_IOMEM
        select AT803X_PHY
        select DAVICOM_PHY
        select SMSC_PHY
        select ICPLUS_PHY
        select REALTEK_PHY
        select NATIONAL_PHY
	---help---
	  Rockchip devices

if NET_VENDOR_ROCKCHIP

source "drivers/net/ethernet/rockchip/vmac/Kconfig"

endif # NET_VENDOR_ROCKCHIP
